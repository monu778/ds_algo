var a = 3;
var p  = new Promise(function(resolve,reject) {
	if(a==2) {

	setTimeout(function() { 
		resolve(10);
         },1000);
        } else {
           reject('fail');

       } 
});

p.then(function(result) {

	console.log(result)
}).catch(function(e) {
 
   console.log("catch",e);
};
        
	

   
